{
    "name": "train",
    "n_gpu": 1,
    "preprocessing": {
        "sr": 22050
    },
    "arch": {
        "type": "MyanFi",
        "args": {
            "input_channels": 80,
            "hidden_channels": 512,
            "upsample_kernels": [16, 16, 4, 4],
            "upsample_stride": [8, 8, 2, 2],
            "resblock_kernels": [3, 7, 11],
            "resblock_dilations": [[1, 3, 5], [1, 3, 5], [1, 3, 5]]
        }
    },
    "data": {
        "train": {
            "batch_size": 5,
            "num_workers": 6,
            "datasets": [
                {
                    "type": "LJspeechDataset",
                    "args": {
                        "data_dir": "/Users/bayesian_monster/tts_project2/data/datasets/ljspeech/wavs",
                        "limit":5
                    }
                }
            ]
        }
    },
    "gen_optimizer": {
        "type": "Adam",
        "args": {
            "lr": 2e-4,
            "weight_decay": 0.0,
            "betas": [0.5, 0.9]
        }
    },
    "dec_optimizer": {
        "type": "Adam",
        "args": {
            "lr": 2e-4,
            "weight_decay": 0.0,
            "betas": [0.5, 0.99]
        }
    },
    "GenLoss": {
        "type": "GenLoss",
        "args": {}
    },
    "DescLoss": {
        "type": "DescLoss",
        "args": {}
    },
    "gen_lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 0.999
        }
    },
    "desc_lr_scheduler": {
        "type": "ExponentialLR",
        "args": {
            "gamma": 0.999
        }
    },
    "trainer": {
        "epochs": 3000,
        "save_dir": "saved/",
        "save_period": 1000,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 500,
        "visualize": "wandb",
        "wandb_project": "MyanFi_project",
        "run_name": "MyanFiOneBatch",
        "sample_rate": 22050,
        "len_epoch": 10,
        "log_step": 50,
        "grad_norm_clip": 10000
    }
}